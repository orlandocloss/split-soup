{
  "expo": {
    "name": "Split Soup",
    "slug": "split-soup",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "icon": "./assets/icon.png",
    "splash": {
      "backgroundColor": "#1a1a1a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.splitsoup.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCalendarsUsageDescription": "Split Soup needs calendar access to help you manage event schedules.",
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "package": "com.splitsoup.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1a1a1a"
      },
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "SCHEDULE_EXACT_ALARM"
      ]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#00D26A",
          "sounds": [],
          "androidMode": "default",
          "androidCollapsedTitle": "Split Soup"
        }
      ],
      "expo-asset"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "eas": {
        "projectId": "f67785e0-07c9-4f46-8e76-9cb59b23c6f5"
      }
    },
    "owner": "orlandocloss",
    "updates": {
      "url": "https://u.expo.dev/f67785e0-07c9-4f46-8e76-9cb59b23c6f5"
    }
  }
}
